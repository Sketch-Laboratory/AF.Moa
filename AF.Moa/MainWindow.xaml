<Window x:Class="AF.Moa.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AF.Moa"
        mc:Ignorable="d"
        Title="AF.Moa" Icon="Resources/icon.png"
        WindowStartupLocation="CenterScreen"
        Width="1200" Height="800">
    <DockPanel>
        <ScrollViewer DockPanel.Dock="Left">
            <StackPanel x:Name="Navigator" Orientation="Vertical" Width="200" Margin="3"></StackPanel>
        </ScrollViewer>
        <WebBrowser x:Name="Browser" DockPanel.Dock="Right"/>
    </DockPanel>
</Window>
